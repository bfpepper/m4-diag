<!DOCTYPE html>
<html>
<head>
  <title>URLockBox</title>
  <%= stylesheet_link_tag    'application', media: 'all'%>
  <%= javascript_include_tag 'application'%>
  <%= csrf_meta_tags %>
</head>
<body>
  <% if current_user %>
  <li><%= link_to "Log Out", logout_path%></li>
  <% else %>
  <li><%= link_to "Log In", login_path %> Or <%= link_to 'Sign Up', new_user_path %> </li>
  <% end %>
  
  <div class="container">
    <%= yield %>
  </div>

</body>
</html>
